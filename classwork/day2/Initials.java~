public class Initials {
    public static void main(Strings[] args) {
	System.out.println("*                 *        *         *");
	System.out.println("* **           ** *        * *       *");
	System.out.println("*    **     **    *        *  *      *");
	System.out.println("*       **        *        *   *     *");
	System.out.println("*                 *        *    *    *");
	System.out.println("*                 *        *     *   *");
	System.out.println("*                 *        *      *  *");
	System.out.println("*                 *        *       * *");
	System.out.println("*                 *        *         *");
    }
}
